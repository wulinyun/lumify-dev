
<% var timeSubtitle = F.vertex.time(vertex),
       subtitle = F.vertex.subtitle(vertex); %>
<li class="<%= classNamesForVertex[vertex.id] || '' %> vertex-item 
            <% if (timeSubtitle) { %> has-timeSubtitle<%}%>
            <% if (subtitle) { %> has-subtitle<%}%>" data-vertex-id="<%= vertex.id %>"
    ><a><%= F.vertex.title(vertex) %>
      <ul>
        <li class="graph"><button title="<%= i18n('vertex.list.in_graph.tooltip') %>" href="#"><%= i18n('vertex.list.in_graph') %></button></li>
        <li class="map"><button title="<%= i18n('vertex.list.in_map.tooltip') %>" href="#"><%= i18n('vertex.list.in_map') %></button></li>
      </ul>
      <div>
        <span class="preview"/>
        <span class="date"><%= timeSubtitle %></span>
        <span class="source"><%= subtitle %></span>
      </div>
    </a>
</li>
